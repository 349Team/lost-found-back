{
  "name": "last-found-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "schema": "./src/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:restart": "npx prisma db pull && npx prisma generate && nodemon src/index.ts",
    "linux:fuser": "fuser -k 3030/tcp && yarn dev:restart",

    "build": "tsc --project .",
    "start": "npx ts-node src/index.ts",
    "restart": "npx prisma db pull && npx prisma generate && yarn start",
    "linux:kill": "fuser -k 3030/tcp",
    "test": "npx ts-node test.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.19",
    "typescript": "^4.8.2"
  }
}
